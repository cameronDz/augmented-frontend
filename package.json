{
  "name": "augmented-frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/v0.2/api": {
      "target": "https://augmentedaspnetbackend.azurewebsites.net/",
      "ws": true
    }
  },
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-intl": "^2.4.0",
    "react-scripts": "1.1.5",
    "FortAwesome/Font-Awesome": "*"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "aws s3 cp --recursive ./build s3://augmented-frontend",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
